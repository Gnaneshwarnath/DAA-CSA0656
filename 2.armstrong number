#include<stdio.h>
int main()
{
	int n,s=0,r=0,temp=0;
	printf("M.Gnaneshwarnath-192211148");
	printf("enter a number:");
	scanf("%d",&n);
	temp=n;
	while(n>0)
	{
		r=n%10;
		s=s+(r*r*r);
		n=n/10;
	}
	if(s==temp)
	{
		printf("it is a armstrong number");
	}
	else
	{
		printf("it is not an armstrong number");
	}
}
