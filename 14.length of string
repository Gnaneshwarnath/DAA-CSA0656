#include<stdio.h>
#include<string.h>
int main()
{

	printf("M.Gnaneshwarnath-192211148\n");
	char str[100];
	int n,i,count=0;
	printf("\n\nEnter string to reverse :");
	scanf("%s", str);
	
	n = strlen(str);
	for(i=0;str[i] != '\0'; i++)
	{
		count += 1;
	}
	printf("Length of the string :%d",count);
}
