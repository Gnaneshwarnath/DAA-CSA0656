#include <stdio.h>
#include <string.h>
int main() 
{
		printf("M.Gnaneshwarnath - 192211148\n");
    char str1[100];
    char str2[100];
    int i;

    printf("\n\nEnter a string: ");
    scanf("%s", str1);
    
    for(i=0; str1[i]!='\0'; i++)
    {
    	str2[i] = str1[i];
	}
	str2[i] = '\0';
	printf("Copied string :%s",str2);
}
